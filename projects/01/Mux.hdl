/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=NotSel);
    And(a=a, b=NotSel, out=AndANotSel);
    And(a=b, b=sel, out=AndASel);
    Or(a=AndANotSel, b=AndASel, out=out);
}
